{
  "name": "pokemon-battle",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:georgetonjr/pokemon-battle.git",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=dev node dist/main/server.js",
    "build": "rm -rf dist && tsc --build tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "dev": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --respawn ./src/main/server.ts",
    "db": "docker-compose -f docker-compose.dev.yml up -d",
    "lint": "npx eslint . --ext .ts --fix",
    "lint:commit": "npx eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "test": "cross-env NODE_ENV=test jest --coverage --passWithNoTests",
    "test:watch": "npm run test -- --watch",
    "test:unit": "cross-env NODE_ENV=test jest --coverage --passWithNoTests --detectOpenHandles --runInBand --testPathPattern=.spec\\.ts",
    "test:integration": "cross-env NODE_ENV=test jest --coverage --passWithNoTests --detectOpenHandles --runInBand --testPathPattern=.test\\.ts",
    "test:specific": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.30.0",
    "swagger-ui-express": "^4.6.2",
    "tedious": "^15.1.3",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "tsyringe": "^4.7.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@jest-mock/express": "^2.0.1",
    "@swc/core": "^1.3.27",
    "@swc/jest": "^0.2.24",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.6",
    "@types/qs": "^6.9.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-boundaries": "^3.1.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "jest-mock-extended": "^3.0.4",
    "joi": "^17.9.1",
    "lint-staged": "^13.1.0",
    "sequelize-cli": "^6.6.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  }
}
